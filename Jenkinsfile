    stages {
        stage('Build') {
            steps {
                echo "building the jar"
            }
        }
        stage('Code_review') {
            steps {
                echo "reviewing the code"
            }
        }
        stage('Deploy-stage') {
            steps {
                echo "deploying stage"
            }
        }
        stage('deploying-prod') {
            steps {
                echo "deploying prod"

            }
        }
    }
}